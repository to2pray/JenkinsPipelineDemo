pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building Stage'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying Stage'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing Stage'
            }
        }
        stage('Release') {
            steps {
                echo 'Releasing Stage'
            }
        }
    }
}
